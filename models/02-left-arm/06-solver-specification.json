{
  "@context": [
    "https://comp-rob2b.github.io/metamodels/task/solver-specification.json",
    {
      "@base": "https://secorolab.github.io/models/demo/"
    }
  ],
  "@id": "https://secorolab.github.io/models/demo/solver-specification.json",
  "@graph": [
    {
      "@id": "spec-frc-leftarm-ee-z",
      "@type": "CartesianForceSpecification",
      "force": "wrench-leftarm-ee-anterior",
      "attached-to": "link-leftarm-ee"
    },
    {
      "@id": "acc-cstr-leftarm-shoulder-ee-lin-y",
      "@type": ["AccelerationConstraint", "AxisAligned"],
      "subspace": "linear-acceleration",
      "axis": "y",
      "acceleration-energy": "eacc-leftarm-shoulder-ee-lin-y"
    },
    {
      "@id": "spec-frc-leftarm-elbow",
      "@type": "CartesianForceSpecification",
      "force": "wrench-leftarm-elbow-elbow",
      "attached-to": "link-leftarm-elbow"
    },
    {
      "@id": "acc-cstr-leftarm-shoulder-ee-ang-x",
      "@type": ["AccelerationConstraint", "AxisAligned"],
      "subspace": "angular-acceleration",
      "axis": "x",
      "acceleration-energy": "eacc-leftarm-shoulder-ee-ang-x"
    },
    {
      "@id": "acc-cstr-leftarm-shoulder-ee-ang-y",
      "@type": ["AccelerationConstraint", "AxisAligned"],
      "subspace": "angular-acceleration",
      "axis": "y",
      "acceleration-energy": "eacc-leftarm-shoulder-ee-ang-y"
    },
    {
      "@id": "acc-cstr-leftarm-shoulder-ee-ang-z",
      "@type": ["AccelerationConstraint", "AxisAligned"],
      "subspace": "angular-acceleration",
      "axis": "z",
      "acceleration-energy": "eacc-leftarm-shoulder-ee-ang-z"
    },
    {
      "@id": "acc-cstr-leftarm-shoulder-ee-lin-z",
      "@type": ["AccelerationConstraint", "AxisAligned"],
      "subspace": "linear-acceleration",
      "axis": "z",
      "acceleration-energy": "eacc-leftarm-shoulder-ee-lin-z"
    },
    {
      "@id": "spec-frc-leftarm-ee",
      "@type": "CartesianForceSpecification",
      "force": "wrench-leftarm-dist-ee",
      "attached-to": "link-leftarm-ee"
    },
    {
      "@id": "spec-acc-leftarm-ee",
      "@type": "AccelerationConstraintSpecification",
      "constraints": [
        "acc-cstr-leftarm-shoulder-ee-lin-y",
        "acc-cstr-leftarm-shoulder-ee-ang-x",
        "acc-cstr-leftarm-shoulder-ee-ang-y",
        "acc-cstr-leftarm-shoulder-ee-ang-z",
        "acc-cstr-leftarm-shoulder-ee-lin-z"
      ],
      "attached-to": "link-leftarm-ee"
    },

    {
      "@id": "prio-leftarm",
      "@type": "PrioritizationLevel",
      "motion-drivers": [
        "spec-acc-leftarm-ee",
        "spec-frc-leftarm-ee-z",
        "spec-frc-leftarm-elbow"
      ]
    },
    {
      "@id": "drv-leftarm",
      "@type": "MotionDrivers",
      "acceleration-constraint": ["spec-acc-leftarm-ee"],
      "cartesian-force": ["spec-frc-leftarm-ee-z", "spec-frc-leftarm-elbow"],
      "joint-force": [],
      "prioritization-hierarchy": ["prio-leftarm"]
    },
    {
      "@id": "slv-leftarm",
      "@type": "SolverWithInputAndOutput",
      "motion-drivers": ["drv-leftarm"],
      "output": ["pose-leftarm-elbow-ee", "twist-leftarm-shoulder-ee"],
      "gravity": "earth-uniform-gravitational-field",
      "solver": "AccelerationConstrainedHybridDynamicsAlgorithm"
    }
  ]
}
