{
    "@context": [
        "https://comp-rob2b.github.io/metamodels/geometry/structural-entities.json",
        "https://comp-rob2b.github.io/metamodels/geometry/spatial-operators.json",
        {
            "@base": "https://secorolab.github.io/models/demo/"
        }
    ],
    "@id": "https://secorolab.github.io/models/demo/",
    "@graph": [
        {
            "@id": "point-rightarm-shoulder-origin",
            "@type": "Point"
        },
        {
            "@id": "point-rightarm-ee-origin",
            "@type": "Point"
        },
        {
            "@id": "frame-rightarm-shoulder",
            "@type": "Frame"
        },
        {
            "@id": "frame-rightarm-elbow",
            "@type": "Frame"
        },
        {
            "@id": "frame-rightarm-ee",
            "@type": "Frame"
        },
        {
            "@id": "link-rightarm-ee",
            "@type": "SimplicialComplex"
        },
        {
            "@id": "link-rightarm-elbow",
            "@type": "SimplicialComplex"
        },
        {
            "@id": "chain-rightarm",
            "@type": "KinematicChain"
        },


        {
            "@id": "frame-platform",
            "@type": "Frame"
        },
        {
            "@id": "link-platform",
            "@type": "SimplicialComplex"
        },


        {
            "@id": "pose-rightarm-to-leftarm-ee",
            "@type": [ "Pose", "PoseCoordinate", "DirectionCosineXYZ", "VectorXYZ" ],
            "of": "frame-leftarm-ee",
            "with-respect-to": "frame-rightarm-ee",
            "quantity-kind": [ "PlaneAngle", "Length" ],
            "as-seen-by": "frame-rightarm-ee",
            "unit": [ "UNITLESS", "M" ]
        },
        {
            "@id": "pose-rightarm-to-leftarm-shoulder",
            "@type": [ "Pose", "PoseCoordinate", "DirectionCosineXYZ", "VectorXYZ" ],
            "of": "frame-leftarm-shoulder",
            "with-respect-to": "frame-rightarm-shoulder",
            "quantity-kind": [ "PlaneAngle", "Length" ],
            "as-seen-by": "frame-rightarm-shoulder",
            "unit": [ "UNITLESS", "M" ]
        },
        {
            "@id": "dir-rightarm-to-leftarm-contact-ee",
            "@type": [ "Quantity", "Direction", "DirectionCoordinate", "VectorXYZ" ],
            "quantity-kind": "Dimensionless",
            "as-seen-by": "frame-rightarm-ee",
            "unit": "UNITLESS"
        },
        {
            "@id": "dir-rightarm-to-leftarm-contact-shoulder",
            "@type": [ "Quantity", "Direction", "DirectionCoordinate", "VectorXYZ" ],
            "quantity-kind": "Dimensionless",
            "as-seen-by": "frame-rightarm-shoulder",
            "unit": "UNITLESS"
        },
        {
            "@id": "rot-dir",
            "@type": "RotateDirectionDistalToProximal",
            "tf": "pose-rightarm-shoulder-ee",
            "from": "dir-rightarm-to-leftarm-contact-ee",
            "to": "dir-rightarm-to-leftarm-contact-shoulder"
        },
        {
            "@id": "dir-rightarm-to-leftarm-shoulder-shoulder",
            "@type": [ "Quantity", "Direction", "DirectionCoordinate", "VectorXYZ" ],
            "quantity-kind": "Dimensionless",
            "as-seen-by": "frame-rightarm-shoulder",
            "unit": "UNITLESS"
        },
        {
            "@id": "ang-table-shoulders",
            "@type": [ "Quantity", "PlaneAngle" ],
            "unit": "RAD"
        },
        {
            "@id": "estimate-angle",
            "@type": "PlanarAngleFromDirections",
            "plane": "horizontal",
            "from-directions": [
                "dir-rightarm-to-leftarm-contact-shoulder",
                "dir-rightarm-to-leftarm-shoulder-shoulder"
            ],
            "angle": "ang-table-shoulders"
        },
        {
            "@id": "ang-to-rightarm",
            "@type": "InvertAngle",
            "in": "ang-table-shoulders",
            "out": "ang-rightarm-shoulder-ee-vertical"
        },



        {
            "@id": "tf-twist-rightarm-shoulder-ee",
            "@type": "RotateVelocityTwistToProximal",
            "tf": "pose-rightarm-platform-shoulder",
            "from": "twist-rightarm-shoulder-ee-shoulder",
            "to": "twist-rightarm-shoulder-ee-platform"
        },
        {
            "@id": "tf-wrench-rightarm-dist",
            "@type": "RotateWrenchToDistal",
            "tf": "pose-rightarm-platform-ee",
            "from": "wrench-rightarm-dist-platform",
            "to": "wrench-rightarm-dist-ee"
        },
        {
            "@id": "tf-wrench-rightarm-elbow",
            "@type": "RotateWrenchToDistal",
            "tf": "pose-rightarm-platform-elbow",
            "from": "wrench-rightarm-elbow-platform",
            "to": "wrench-rightarm-elbow-elbow"
        },
        {
            "@id": "tf-pose-rightarm-shoulder-elbow",
            "@type": "ComposePose",
            "in1": "pose-rightarm-platform-shoulder",
            "in2": "pose-rightarm-shoulder-elbow",
            "out": "pose-rightarm-platform-elbow"
        },
        {
            "@id": "tf-pose-rightarm-shoulder-ee",
            "@type": "ComposePose",
            "in1": "pose-rightarm-platform-shoulder",
            "in2": "pose-rightarm-shoulder-ee",
            "out": "pose-rightarm-platform-ee"
        }
    ]
}
