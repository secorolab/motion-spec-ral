{
    "@context": [
        "https://comp-rob2b.github.io/metamodels/task/view.json",
        "https://comp-rob2b.github.io/metamodels/geometry/spatial-operators.json",
        {
            "@base": "https://secorolab.github.io/models/demo/"
        }
    ],
    "@id": "https://secorolab.github.io/models/demo/",
    "@graph": [
        {
            "@id": "view-frc-rightarm-ee-anteroposterior",
            "@type": [ "View", "WrenchCoordinateView" ],
            "superobject": "wrench-rightarm-ee-anteroposterior",
            "subobject": "frc-rightarm-ee-anteroposterior",
            "subspace": "force",
            "axis": "x"
        },
        {
            "@id": "view-linvel-rightarm-shoulder-ee-vertical",
            "@type": [ "View", "VelocityTwistCoordinateView" ],
            "superobject": "twist-rightarm-shoulder-ee-platform",
            "subobject": "linvel-rightarm-shoulder-ee-vertical",
            "subspace": "linear-velocity",
            "axis": "z"
        },



        {
            "@id": "view-pos-rightarm-shoulder-elbow-height",
            "@type": [ "View", "PoseCoordinateView" ],
            "superobject": "pose-rightarm-platform-elbow",
            "subobject": "pos-rightarm-platform-elbow-height",
            "subspace": "position",
            "axis": "z"
        },
        {
            "@id": "view-frc-rightarm-elbow",
            "@type": [ "View", "WrenchCoordinateView" ],
            "superobject": "wrench-rightarm-elbow-platform",
            "subobject": "frc-rightarm-elbow",
            "subspace": "force",
            "axis": "z"
        },



        {
            "@id": "view-angvel-rightarm-shoulder-ee-anteroposterior",
            "@type": [ "View", "VelocityTwistCoordinateView" ],
            "superobject": "twist-rightarm-shoulder-ee-platform",
            "subobject": "angvel-rightarm-shoulder-ee-anteroposterior",
            "subspace": "angular-velocity",
            "axis": "x"
        },
        {
            "@id": "view-angvel-rightarm-shoulder-ee-lateral",
            "@type": [ "View", "VelocityTwistCoordinateView" ],
            "superobject": "twist-rightarm-shoulder-ee-platform",
            "subobject": "angvel-rightarm-shoulder-ee-lateral",
            "subspace": "angular-velocity",
            "axis": "y"
        },
        {
            "@id": "pose-to-ang-rightarm-shoulder-ee-vertical",
            "@type": "PoseToAngleAroundAxis",
            "pose": "pose-rightarm-platform-ee",
            "angle": "ang-rightarm-shoulder-ee-vertical",
            "axis": "z"
        },
        {
            "@id": "view-linvel-rightarm-shoulder-ee-lateral",
            "@type": [ "View", "VelocityTwistCoordinateView" ],
            "superobject": "twist-rightarm-shoulder-ee-platform",
            "subobject": "linvel-rightarm-shoulder-ee-lateral",
            "subspace": "linear-velocity",
            "axis": "y"
        },



        {
            "@id": "pose-to-dist-rightarm",
            "@type": "PoseToLinearDistance",
            "pose": "pose-rightarm-shoulder-ee",
            "distance": "dist-rightarm-shoulder-ee"
        },
        {
            "@id": "pose-to-dir-rightarm",
            "@type": "PoseToDirection",
            "pose": "pose-rightarm-shoulder-ee",
            "direction": "dir-rightarm-shoulder-to-ee"
        },
        {
            "@id": "compute-wrench-rightarm-dist",
            "@type": "PureForceFromDirectionAndMagnitude",
            "magnitude": "frc-rightarm-dist",
            "direction": "dir-rightarm-shoulder-to-ee",
            "wrench": "wrench-rightarm-dist-platform"
        }
    ]
}
