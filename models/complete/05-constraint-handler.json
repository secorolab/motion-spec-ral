{
    "@context": [
        "https://comp-rob2b.github.io/metamodels/qudt-quant.json",
        "https://comp-rob2b.github.io/metamodels/task/constraint-handler.json",
        {
            "@base": "https://secorolab.github.io/models/demo/"
        }
    ],
    "@id": "https://secorolab.github.io/models/demo/",
    "@graph": [
        {
            "@id": "eval-frc-rightarm-ee-anteroposterior",
            "@type": [ "ConstraintEvaluator", "AssignmentEvaluator" ],
            "constraint": "cstr-frc-rightarm-ee-anteroposterior"
        },

        {
            "@id": "eval-linvel-rightarm-shoulder-ee-vertical",
            "@type": [ "ConstraintEvaluator", "ErrorEvaluator" ],
            "constraint": "cstr-linvel-rightarm-shoulder-ee-vertical",
            "error": "linvel-rightarm-shoulder-ee-vertical-err"
        },
        {
            "@id": "ctrl-linvel-rightarm-shoulder-ee-vertical",
            "@type": [ "Controller", "ProportionalIntegralDerivative" ],
            "error-signal": "linvel-rightarm-shoulder-ee-vertical-err",
            "control-signal": "eacc-rightarm-shoulder-ee-lin-z",
            "proportional-gain": "5.0",
            "integral-gain": "1.0",
            "derivative-gain": "3.0"
        },

        {
            "@id": "eval-pos-rightarm-platform-elbow-height",
            "@type": [ "ConstraintEvaluator", "ErrorEvaluator" ],
            "constraint": "cstr-pos-rightarm-platform-elbow-height",
            "error": "pos-rightarm-platform-elbow-height-err"
        },
        {
            "@id": "ctrl-pos-rightarm-platform-elbow-height",
            "@type": [ "Controller", "ProportionalIntegralDerivative" ],
            "error-signal": "pos-rightarm-platform-elbow-height-err",
            "control-signal": "frc-rightarm-elbow",
            "proportional-gain": "100.0",
            "integral-gain": "0.0",
            "derivative-gain": "0.0"
        },

        {
            "@id": "eval-angvel-rightarm-shoulder-ee-anteroposterior",
            "@type": [ "ConstraintEvaluator", "ErrorEvaluator" ],
            "constraint": "cstr-angvel-rightarm-shoulder-ee-anteroposterior",
            "error": "angvel-rightarm-shoulder-ee-anteroposterior-err"
        },
        {
            "@id": "ctrl-angvel-rightarm-shoulder-ee-anteroposterior",
            "@type": [ "Controller", "ProportionalIntegralDerivative" ],
            "error-signal": "angvel-rightarm-shoulder-ee-anteroposterior-err",
            "control-signal": "eacc-rightarm-shoulder-ee-ang-x",
            "proportional-gain": "5.0",
            "integral-gain": "1.0",
            "derivative-gain": "3.0"
        },

        {
            "@id": "eval-angvel-rightarm-shoulder-ee-lateral",
            "@type": [ "ConstraintEvaluator", "ErrorEvaluator" ],
            "constraint": "cstr-angvel-rightarm-shoulder-ee-lateral",
            "error": "angvel-rightarm-shoulder-ee-lateral-err"
        },
        {
            "@id": "ctrl-angvel-rightarm-shoulder-ee-lateral",
            "@type": [ "Controller", "ProportionalIntegralDerivative" ],
            "error-signal": "angvel-rightarm-shoulder-ee-lateral-err",
            "control-signal": "eacc-rightarm-shoulder-ee-ang-y",
            "proportional-gain": "5.0",
            "integral-gain": "1.0",
            "derivative-gain": "3.0"
        },

        {
            "@id": "eval-ang-rightarm-shoulder-ee-vertical",
            "@type": [ "ConstraintEvaluator", "ErrorEvaluator" ],
            "constraint": "cstr-ang-rightarm-shoulder-ee-vertical",
            "error": "ang-rightarm-shoulder-ee-vertical-err"
        },
        {
            "@id": "ctrl-ang-rightarm-shoulder-ee-vertical",
            "@type": [ "Controller", "ProportionalIntegralDerivative" ],
            "error-signal": "ang-rightarm-shoulder-ee-vertical-err",
            "control-signal": "eacc-rightarm-shoulder-ee-ang-z",
            "proportional-gain": "200.0",
            "integral-gain": "2.9",
            "derivative-gain": "80.5"
        },

        {
            "@id": "eval-linvel-rightarm-shoulder-ee-lateral",
            "@type": [ "ConstraintEvaluator", "ErrorEvaluator" ],
            "constraint": "cstr-linvel-rightarm-shoulder-ee-lateral",
            "error": "linvel-rightarm-shoulder-ee-lateral-err"
        },
        {
            "@id": "ctrl-linvel-rightarm-shoulder-ee-lateral",
            "@type": [ "Controller", "ProportionalIntegralDerivative" ],
            "error-signal": "linvel-rightarm-shoulder-ee-lateral-err",
            "control-signal": "eacc-rightarm-shoulder-ee-lin-y",
            "proportional-gain": "5.0",
            "integral-gain": "1.0",
            "derivative-gain": "3.0"
        },

        {
            "@id": "eval-dist-rightarm-shoulder-ee",
            "@type": [ "ConstraintEvaluator", "ErrorEvaluator" ],
            "constraint": "cstr-dist-rightarm-shoulder-ee",
            "error": "dist-rightarm-shoulder-ee-err"
        },
        {
            "@id": "ctrl-dist-rightarm-shoulder-ee",
            "@type": [ "Controller", "ProportionalIntegralDerivative", "DecayingIntegralTerm" ],
            "error-signal": "dist-rightarm-shoulder-ee-err",
            "control-signal": "frc-rightarm-dist",
            "proportional-gain": "450.0",
            "integral-gain": "65.5",
            "derivative-gain": "80.0",
            "decay-rate": "0.99"
        },

        {
            "@id": "cstr-hdl-rightarm",
            "@type": "ConstraintHandler",
            "motion": "motion-rightarm",
            "evaluators": [
                "eval-frc-rightarm-ee-anteroposterior",
                "eval-linvel-rightarm-shoulder-ee-vertical",
                "eval-pos-rightarm-platform-elbow-height",
                "eval-angvel-rightarm-shoulder-ee-anteroposterior",
                "eval-angvel-rightarm-shoulder-ee-lateral",
                "eval-ang-rightarm-shoulder-ee-vertical",
                "eval-linvel-rightarm-shoulder-ee-lateral",
                "eval-dist-rightarm-shoulder-ee"
            ],
            "monitors": [],
            "controllers": [
                "ctrl-linvel-rightarm-shoulder-ee-vertical",
                "ctrl-pos-rightarm-platform-elbow-height",
                "ctrl-angvel-rightarm-shoulder-ee-anteroposterior",
                "ctrl-angvel-rightarm-shoulder-ee-lateral",
                "ctrl-ang-rightarm-shoulder-ee-vertical",
                "ctrl-linvel-rightarm-shoulder-ee-lateral",
                "ctrl-dist-rightarm-shoulder-ee"
            ]
        }
    ]
}
